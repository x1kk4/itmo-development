{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "auto",
  "singleAttributePerLine": true,
  "importOrder": [
    ".*components/(.*)$",
    ".*redux/(.*)$|.*hoc/(.*)$|.*hooks(.*)$",
    ".*types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "insertFinalNewline": true
}

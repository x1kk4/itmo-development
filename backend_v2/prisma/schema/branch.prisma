model Branch {
  id              Int @id @default(autoincrement())
  name            String @unique
  location        String @unique
  workingStart    String
  workingEnd      String
  contactPhone    String
  contactEmail    String
  contactTelegram String?
  yaMapsLink      String? @unique
  twogisLink      String? @unique
  gMapsLink       String? @unique
  photos          String[]

  trainingSessions TrainingSession[]
}